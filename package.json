{
  "author": "Redis Labs",
  "description": "Redis Application for Grafana",
  "devDependencies": {
    "@grafana/data": "7.4.2",
    "@grafana/runtime": "7.4.2",
    "@grafana/toolkit": "7.4.2",
    "@grafana/ui": "7.4.2",
    "@monaco-editor/react": "^4.0.10",
    "@types/enzyme": "^3.10.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "emotion": "10.0.27",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "Apache-2.0",
  "name": "redis-app",
  "scripts": {
    "build": "grafana-toolkit plugin:build --coverage",
    "dev": "grafana-toolkit plugin:dev",
    "format": "prettier --write \"**\"",
    "sign": "grafana-toolkit plugin:sign",
    "start": "docker-compose up",
    "start:dev": "docker-compose -f docker-compose/dev.yml up",
    "stop": "docker-compose down",
    "stop:dev": "docker-compose -f docker-compose/dev.yml down",
    "test": "grafana-toolkit plugin:test --coverage",
    "upgrade": "yarn upgrade --latest",
    "watch": "grafana-toolkit plugin:dev --watch"
  },
  "version": "1.2.0"
}
